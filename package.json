{
  "name": "remix-template-cloudflare-workers",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build dist public/build",
    "prebuild": "npm run clean",
    "build": "remix build && ts-node build-worker",
    "deploy": "npm run build && wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev-old": "remix build && run-p dev:*",
    "dev": "npm run build && wrangler dev --local",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.4.3",
    "@remix-run/cloudflare-workers": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/server-runtime": "^1.4.3",
    "cross-env": "^7.0.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "remix-cloudflare-workers-fetch": "^1.0.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.10.0",
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "esbuild": "^0.14.39",
    "eslint": "^8.15.0",
    "miniflare": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "wrangler": "^2.0.5"
  },
  "engines": {
    "node": ">=14"
  }
}
